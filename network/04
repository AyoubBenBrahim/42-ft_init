eval netstat -rn | awk -F" " '(NR == 5) {print $2}'

# eval route get default | grep gateway
# traceroute 1337.ma

# -r      Bypass the normal routing tables and send directly to a host on an attached network.

# -n      Print hop addresses numerically rather than symbolically and numerically (saves a nameserver
             address-to-name lookup for each gateway found on the path).
